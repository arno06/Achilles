{
	"front":
	{
		"/":
		{
			"*":
			{
				"controller":"index",
				"action":"index"
			}
		},
		"sign-in":
		{
			"GET|POST":
			{
				"controller":"user",
				"action":"sign_in"
			}
		},
		"sign-out":
		{
			"GET":
			{
				"controller":"user",
				"action":"sign_out"
			}
		},
		"register":
		{
			"GET|POST":
			{
				"controller":"user",
				"action":"register"
			}
		},
		"submit":
		{
			"GET|POST":
			{
				"controller":"post",
				"action":"submit"
			}
		},
		"post/scrap":
		{
			"GET|POST":
			{
				"controller":"post",
				"action":"scrap"
			}
		},
		"post/{$permalink}":
		{
			"parameters":
			{
				"permalink":"[a-zA-Z0-9\\-\\_]{16}"
			},
			"GET|POST":
			{
				"controller":"post",
				"action":"view"
			}
		},
		"profil/{$pseudo_user}":
		{
			"parameters":
			{
				"pseudo_user":"[a-zA-Z0-9\\_\\-]+"
			},
			"GET":
			{
				"controller":"user",
				"action":"profil"
			}
		},
		"profil/{$pseudo_user}/avatar":
		{
			"parameters":
			{
				"pseudo_user":"[a-zA-Z0-9\\_\\-]+"
			},
			"GET":
			{
				"controller":"user",
				"action":"avatar"
			}
		},
		"profil/{$pseudo_user}/edit":
		{
			"parameters":
			{
				"pseudo_user":"[a-zA-Z0-9\\_\\-]+"
			},
			"GET|POST":
			{
				"controller":"user",
				"action":"edit"
			}
		}
	}
}