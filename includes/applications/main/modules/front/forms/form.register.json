{
	"email_user":
	{
		"label":"Email",
		"require":true,
		"regExp":"mail",
		"tag":"input",
		"attributes":
		{
			"placeholder":"Email",
			"type":"text"
		}
	},
	"password_user":
	{
		"label":"Password",
		"require":true,
		"regExp":"textNoHtml",
		"tag":"input",
		"attributes":
		{
			"placeholder":"Password",
			"type":"password"
		}
	},
	"pseudo_user":
	{
		"label":"Pseudo",
		"require":true,
		"regExp":"[a-zA-Z0-9\\_\\-]+",
		"tag":"input",
		"attributes":
		{
			"placeholder":"Pseudo",
			"type":"text"
		}
	},
	"submit":
	{
		"tag":"input",
		"attributes":
		{
			"type":"submit",
			"value":"Register"
		}
	}
}